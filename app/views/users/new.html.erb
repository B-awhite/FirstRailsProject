<h1>Sign Up</h1>

<%= render partial: "/layouts/errors", locals: {object: @user} %>

<%= form_for(@user, url:'/signup') do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Signup" %>

<% end %>